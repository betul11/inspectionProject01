<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.addEquipmentController">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="700.0">
            <children>
                <AnchorPane prefHeight="98.0" prefWidth="700.0" style="-fx-background-color: #54afbc;">
                    <children>
                        <Label alignment="CENTER" layoutX="2.0" layoutY="31.0" prefHeight="18.0" prefWidth="170.0" text="Add Equipment" textFill="#dad5d5">
                            <font>
                                <Font size="20.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <JFXTextField fx:id="equipmentNameText" layoutX="132.0" layoutY="134.0" prefHeight="26.0" prefWidth="164.0" promptText="Equipment" />
                <JFXTextField fx:id="equipmentPoleDistanceText" layoutX="132.0" layoutY="206.0" prefHeight="26.0" prefWidth="164.0" promptText="Pole Distance" />
                <JFXTextField fx:id="equipmentLightDistanceText" layoutX="363.0" layoutY="134.0" prefHeight="26.0" prefWidth="164.0" promptText="Distance of Light" />
                <JFXButton fx:id="equipmentSaveButton" layoutX="555.0" layoutY="17.0" prefHeight="26.0" prefWidth="115.0" style="-fx-background-color: #fe5c36;" text="Save" />
                <JFXTextField fx:id="equipmentCarrierMediumText" layoutX="132.0" layoutY="277.0" prefHeight="26.0" prefWidth="164.0" promptText="MP Carrier Medium" />
                <JFXButton fx:id="equipmentCancelButton" layoutX="555.0" layoutY="47.0" prefHeight="26.0" prefWidth="115.0" style="-fx-background-color: #434343;" text="Cancel" textFill="#f0ecec" />
            <JFXTextField fx:id="equipmentMagTechText" layoutX="363.0" layoutY="204.0" prefHeight="26.0" prefWidth="164.0" promptText="Mag. Tech." />
            <JFXTextField fx:id="equipmentLightIntensityText" layoutX="363.0" layoutY="274.0" prefHeight="26.0" prefWidth="164.0" promptText="UV Light Intensity" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
